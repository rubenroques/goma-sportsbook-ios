# Match Configuration (single repo for all environments)
MATCH_GIT_URL=git@github.com:gomagaming/FastlaneAppleCertificates.git
MATCH_PASSWORD=your_match_encryption_password
MATCH_KEYCHAIN_PASSWORD=your_keychain_password

# App Store Connect API Key (for UAT - Goma team)
# Benefits: No 2FA prompts, better for CI/CD, more secure
# Get from: https://appstoreconnect.apple.com/access/api
APP_STORE_CONNECT_API_KEY_ID=your_key_id
APP_STORE_CONNECT_API_ISSUER_ID=your_issuer_id
APP_STORE_CONNECT_API_KEY_FILEPATH=./fastlane/AuthKey_XXXXXXXXXX.p8

# Firebase Distribution
FIREBASE_UAT_DISTRIBUTION_GROUPS=ios-internal
FIREBASE_PROD_DISTRIBUTION_GROUPS=ios-internal
FIREBASE_DISTRIBUTION_TESTERS=your_email@example.com

# Firebase CLI Tokens (get via: firebase login:ci)
FIREBASE_UAT_CLI_TOKEN=your_firebase_uat_token
FIREBASE_PROD_CLI_TOKEN=your_firebase_prod_token

